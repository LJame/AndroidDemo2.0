<?xml version="1.0" encoding="utf-8"?>
<manifest package="ad2.hardrubic.com.androiddemo20"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.hardrubic.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.hardrubic.activity.ScrollActivity">
        </activity>
        <activity android:name="com.hardrubic.activity.HorizontalScrollActivity">
        </activity>

        <service
            android:name="com.hardrubic.remote.service.RemoteService"
            android:process=":remote"/>

        <activity
            android:name="com.hardrubic.activity.AidlActivity"
            android:theme="@style/AppTheme">
        </activity>
        <activity android:name="com.hardrubic.activity.RecyclerViewActivity">
        </activity>
        <activity android:name="com.hardrubic.activity.ThreadTestActivity">
        </activity>
        <activity
            android:name="com.hardrubic.activity.CustomViewActivity"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>
